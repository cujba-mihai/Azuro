    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail --passWithNoTests  "
    ]
  },
  "jest": {
    "preset": "react-native"
  },
  "private": true,
  "volta": {
    "node": "20.10.0",
    "yarn": "4.0.2"
  }
