    "format": "prettier --write \"**/*.{tsx,jsx,ts,js}\"",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint . & yarn run typecheck",
    "typecheck": "tsc --noEmit --allowJs"
  },
  "dependencies": {
    "@expo-google-fonts/comfortaa": "^0.2.3",
    "@expo-google-fonts/inter": "^0.2.3",
    "@svgr/plugin-prettier": "^8.1.0",
    "expo": "^49.0.21",
    "expo-image": "1.5.1",
    "expo-linking": "~5.0.2",
